{
	// Required
	"name" : "Read Later",
	"version" : "1.0",
	"description" : "Replaces the instapaper bookmarklet",
	"icons" : {
		"128" : "instapaper.png",
		"48" : "instapaper.png"
	},
	// Actions
	"browser_action" : {
		"default_icon" : "instapaper.png",
		"default_title" : "Save to instapaper"
	},
	// Other
	"background" : "read-later.html",
	"options_page" : "read-later.options.html",
	"permissions" : [
		"tabs",
		"http://*/*"
	],
	"manifest_version": 2
}
